<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lcc.basic.entity">
    <class name="com.lcc.basic.test.Staff" table="t_staff">
        <id name="id">
            <generator class="increment"/>
        </id>
        <property name="name"/>
        <many-to-one name="dept" column="deptId" class="com.lcc.basic.test.Dept"/>
    </class>
</hibernate-mapping>
