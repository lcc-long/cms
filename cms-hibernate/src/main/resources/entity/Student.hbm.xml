<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lcc.basic.entity">
    <class name="com.lcc.basic.test.Student" table="t_stu">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="name" column="sname"/>
        <set name="teachers" table="stu_tea">
            <key column="s_id"></key>
            <many-to-many class="com.lcc.basic.test.Teacher" column="t_id"/>
        </set>
    </class>
</hibernate-mapping>
